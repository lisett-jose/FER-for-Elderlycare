<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.elderlycare"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/elderlyicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.elderlycare.Ipsettings"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.elderlycare.Login" >
        </activity>
        <activity android:name="com.example.elderlycare.RegisterFamilyMember" >
        </activity>
        <activity android:name="com.example.elderlycare.UserHome" >
        </activity>
        <activity android:name="com.example.elderlycare.ViewEmergency" >
        </activity>
        <activity android:name="com.example.elderlycare.View_notifications" >
        </activity>

        <service android:name="com.example.elderlycare.notification" >
        </service>

        <activity
            android:name="com.example.elderlycare.AdminHome"
            android:label="@string/title_activity_admin_home" >
        </activity>
        <activity
            android:name="com.example.elderlycare.NewUser"
            android:label="@string/title_activity_new_user" >
        </activity>
    </application>

</manifest>
